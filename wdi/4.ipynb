    "def modified_bubblesort(T):\n",
    "    n = len(T)\n",
    "    for i in range(n):\n",
    "        for j in range(n-1):\n",
    "            if T[j] == 0:\n",
    "                T[j], T[j+1] = T[j+1], T[j]\n",
    "    return T\n",
    "\n",
    "def merge_with_twist(T):\n",
    "    n = len(T)\n",
    "    T2 = [0] * (n**2)\n",
    "    powt = [0] * (n**2 // 2)\n",
    "    powt_wsk = 0\n",
    "    wsk = [0] * n\n",
    "    i = 0\n",
    "    while i < n ** 2:\n",
    "        min_elem = float(\"inf\")\n",
    "        min_j = -1\n",
    "        for j in range(n):\n",
    "            if wsk[j] < n and T[j][wsk[j]] < min_elem:\n",
    "                if T[j][wsk[j]] == T2[i-1]:\n",
    "                    powt[powt_wsk] = T2[i-1]\n",
    "                    powt_wsk += 1\n",
    "                min_elem = T[j][wsk[j]]\n",
    "                min_j = j\n",
    "        T2[i] = min_elem\n",
    "        wsk[min_j] += 1\n",
    "        i += 1\n",
    "    \n",
    "    \n",
    "    for i in range(n**2):\n",
    "        if T2[i] in powt: \n",
    "            T2[i] = 0\n",
    "            \n",
    "    return modified_bubblesort(T2)\n",
    "    \n",
    "\n",
