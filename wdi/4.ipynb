    "def adjacent_elements(T, i, j): \n",
    "    n = len(T) - 1 # indeksy <0, k-1>\n",
    "    main_set = set_of_digits(T[i][j])\n",
    "    main_flag = True\n",
    "    \n",
    "    if 0 <= j+1 <= n: # right\n",
    "        if set_of_digits(T[i][j+1]) != main_set:\n",
    "            main_flag = False\n",
    "        \n",
    "    if 0 <= j-1 <= n: # left\n",
    "        if set_of_digits(T[i][j-1]) != main_set:\n",
    "            main_flag = False\n",
    "        \n",
    "    if 0 <= i-1 <= n: # upper\n",
    "        if set_of_digits(T[i-1][j]) != main_set:\n",
    "            main_flag = False\n",
    "    \n",
    "    if 0 <= i+1 <= n: # lower\n",
    "        if set_of_digits(T[i+1][j]) != main_set:\n",
    "            main_flag = False\n",
    "    return main_flag\n",
    "    \n",
    "def set_of_digits(num):\n",
    "    digits = [0]*10\n",
    "    while num > 0:\n",
